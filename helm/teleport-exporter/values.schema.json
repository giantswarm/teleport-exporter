                }
            }
        },
        "image": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            }
        },
        "imagePullSecrets": {
            "type": "array"
        },
        "global": {
            "type": "object",
            "properties": {
                "podSecurityStandards": {
                    "type": "object",
                    "properties": {
