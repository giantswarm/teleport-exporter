            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "containerPolicies": {
                    "type": "object",
                    "properties": {
                        "minAllowed": {
                            "type": "object",
                            "properties": {
                                "cpu": {
                                    "type": "string"
                                },
                                "memory": {
                                    "type": "string"
                                }
                            }
